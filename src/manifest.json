{
  "manifest_version": 2,
  "name": "Unrounded Twitter",
  "short_name": "Unrounded Twitter",
  "version": "${version}",
  "description": "${description}",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": ["/css/main.css"]
    }
  ],
  "browser_action": {
    "default_icon": "./img/icon128.png"
  },
  "icons": {
    "128": "./img/icon128.png"
  },
  "homepage_url": "${homepage}",
  "options_page": "options.html"
}